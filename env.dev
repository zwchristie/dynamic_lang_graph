# Development Environment Configuration
ENVIRONMENT=dev

# Provider selection
PROVIDER=openai

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Bedrock
BEDROCK_REGION=us-east-1
BEDROCK_ACCESS_KEY=your_aws_access_key
BEDROCK_SECRET_KEY=your_aws_secret_key
BEDROCK_LLM_MODEL_ID=your-bedrock-llm-model-id
BEDROCK_EMBEDDING_MODEL_ID=your-bedrock-embedding-model-id

# Database
DATABASE_URL=sqlite:///./app_dev.db

# App
APP_NAME=Agentic Workflow System (Development)
DEBUG=True
MAX_ITERATIONS=10

# Server settings (with SSL for dev)
HOST=0.0.0.0
PORT=8443

# SSL settings (for development)
SSL_CERT_FILE=./certs/dev_cert.pem
SSL_KEY_FILE=./certs/dev_key.pem 