# Testing Environment Configuration
ENVIRONMENT=testing

# Provider selection
PROVIDER=openai

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Bedrock
BEDROCK_REGION=us-east-1
BEDROCK_ACCESS_KEY=your_aws_access_key
BEDROCK_SECRET_KEY=your_aws_secret_key
BEDROCK_LLM_MODEL_ID=your-bedrock-llm-model-id
BEDROCK_EMBEDDING_MODEL_ID=your-bedrock-embedding-model-id

# Database
DATABASE_URL=sqlite:///./app_test.db

# App
APP_NAME=Agentic Workflow System (Testing)
DEBUG=True
MAX_ITERATIONS=10

# Server settings (with SSL for testing)
HOST=0.0.0.0
PORT=8444

# SSL settings (for testing)
SSL_CERT_FILE=./certs/test_cert.pem
SSL_KEY_FILE=./certs/test_key.pem 